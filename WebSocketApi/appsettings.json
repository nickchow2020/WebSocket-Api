{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "WebSocketApi": "Information"
    }
  },
  "AllowedHosts": "*",
  "CorsSettings": {
    "AllowedOrigins": [
      "http://localhost:3000"
    ]
  },
  "WebSocketSettings": {
    "KeepAliveIntervalSeconds": 30,
    "ReceiveBufferSize": 16384,
    "MaxMessageSize": 1048576,
    "ReceiveTimeoutSeconds": 120,
    "IdleTimeoutSeconds": 300
  }
}
